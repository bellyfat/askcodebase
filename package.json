{
  "name": "askcodebase",
  "displayName": "AskCodebase AI",
  "description": "Chat is All You Need! Write Code 100x faster.",
  "version": "0.1.2",
  "engines": {
    "vscode": "^1.70.0"
  },
  "icon": "media/logo.png",
  "publisher": "JipitiAI",
  "pricing": "Free",
  "repository": "https://github.com/jipitiai/vscode-ask-codebase",
  "license": "UNLICENSED",
  "categories": [
    "Snippets",
    "Programming Languages",
    "Debuggers",
    "Machine Learning",
    "Education"
  ],
  "keywords": [
    "ai",
    "autocomplete",
    "code-recommendation",
    "intellisense",
    "intellicode",
    "typescript",
    "javascript",
    "bash",
    "c",
    "c#",
    "c++",
    "cpp",
    "csharp"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ask-codebase.helloWorld",
        "title": "Hello World"
      }
    ],
    "viewsContainers": {
      "panel": [
        {
          "icon": "media/dep.svg",
          "id": "ask-codebase",
          "title": "Ask Codebase"
        }
      ]
    },
    "views": {
      "ask-codebase": [
        {
          "type": "webview",
          "id": "ask-codebase",
          "name": "Ask Codebase",
          "icon": "media/dep.svg"
        }
      ]
    }
  },
  "scripts": {
    "rebuild": "electron-rebuild -v 22.3.14",
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "npm run build && webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "dev": "NODE_ENV=development webpack serve --config webpack.client.js",
    "build": "NODE_ENV=production webpack --config webpack.client.js"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.2.13",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@svgr/webpack": "^8.0.1",
    "@tailwindcss/typography": "^0.5.9",
    "@types/fs-extra": "^11.0.1",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/react": "^18.2.14",
    "@types/react-syntax-highlighter": "^15.5.7",
    "@types/uuid": "^9.0.2",
    "@types/vscode": "^1.70.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "autoprefixer": "^10.4.14",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "grapheme-splitter": "^1.0.4",
    "mocha": "^10.2.0",
    "node-loader": "^2.0.0",
    "node-pty": "^1.0.0",
    "postcss-loader": "^7.3.3",
    "prettier": "^2.8.8",
    "react-refresh": "^0.14.0",
    "sass": "^1.63.6",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.3.3",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.85.0",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.1",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@askcodebase/common": "^1.1.0",
    "@hediet/node-reload": "^0.7.3",
    "@hediet/std": "^0.6.0",
    "@monaco-editor/react": "^4.5.1",
    "@tabler/icons-react": "^2.30.0",
    "@types/react-dom": "^18.2.6",
    "@vscode/codicons": "^0.0.33",
    "@vscode/webview-ui-toolkit": "^1.2.2",
    "classnames": "^2.3.2",
    "events": "^3.3.0",
    "jotai": "^2.2.2",
    "jotai-devtools": "^0.6.0",
    "monaco-editor": "^0.39.0",
    "node-fetch": "^3.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.7",
    "react-syntax-highlighter": "^15.5.0",
    "react-tooltip": "^5.18.0",
    "rehype-mathjax": "^4.0.3",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "uuid": "^9.0.0",
    "xterm": "^5.2.1"
  }
}
