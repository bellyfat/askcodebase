{
  "name": "askcodebase",
  "displayName": "AskCodebase AI",
  "description": "Chat is All You Need. Write Code 100x faster.",
  "version": "0.17.0",
  "engines": {
    "vscode": "^1.70.0"
  },
  "icon": "media/logo.png",
  "publisher": "JipitiAI",
  "pricing": "Free",
  "repository": "https://github.com/jipitiai/vscode-ask-codebase",
  "license": "UNLICENSED",
  "categories": [
    "Programming Languages",
    "Machine Learning",
    "Snippets",
    "Debuggers",
    "Education"
  ],
  "keywords": [
    "Conversation Programming",
    "Chat Is All You Need",
    "AI Code Assistant",
    "ChatGPT",
    "GPT-4",
    "GPT-3.5-Turbo",
    "GPT-3.5",
    "GPT4",
    "Copilot"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "walkthroughs": [
      {
        "id": "askcodebase-walkthrough",
        "title": "AskCodebase AI",
        "description": "A sample walkthrough",
        "steps": [
          {
            "id": "runcommand",
            "title": "Run Command",
            "description": "This step will run a command and check off once it has been run.\n[Run Command](command:getting-started-sample.runCommand)",
            "media": {
              "image": "media/image.png",
              "altText": "Empty image"
            },
            "completionEvents": [
              "onCommand:getting-started-sample.runCommand"
            ]
          },
          {
            "id": "changesetting",
            "title": "Change Setting",
            "description": "This step will change a setting and check off when the setting has changed\n[Change Setting](command:getting-started-sample.changeSetting)",
            "media": {
              "markdown": "media/markdown.md"
            },
            "completionEvents": [
              "onSettingChanged:getting-started-sample.sampleSetting"
            ]
          }
        ]
      }
    ],
    "keybindings": [
      {
        "command": "askcodebase.toggleAskCodebase",
        "key": "ctrl+h",
        "mac": "cmd+h"
      }
    ],
    "commands": [
      {
        "command": "askcodebase.toggleAskCodebase",
        "title": "AskCodebase: Toggle AskCodebase Panel"
      }
    ],
    "viewsContainers": {
      "panel": [
        {
          "icon": "media/logo.svg",
          "id": "ask-codebase",
          "title": "AskCodebase"
        }
      ]
    },
    "views": {
      "ask-codebase": [
        {
          "type": "webview",
          "id": "ask-codebase",
          "name": "AskCodebase",
          "icon": "media/logo.svg"
        }
      ]
    }
  },
  "scripts": {
    "rebuild": "electron-rebuild -v 22.3.14",
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "npm run build:client && npx webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "dev": "NODE_ENV=development npx webpack serve --config webpack.client.js",
    "build:client": "NODE_ENV=production npx webpack --config webpack.client.js",
    "pub": "node scripts/publish.js"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.2.13",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@svgr/webpack": "^8.0.1",
    "@tailwindcss/typography": "^0.5.9",
    "@types/fs-extra": "^11.0.1",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/react": "^18.2.14",
    "@types/react-syntax-highlighter": "^15.5.7",
    "@types/uuid": "^9.0.2",
    "@types/vscode": "^1.70.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "autoprefixer": "^10.4.14",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "grapheme-splitter": "^1.0.4",
    "mocha": "^10.2.0",
    "node-loader": "^2.0.0",
    "node-pty": "^1.0.0",
    "postcss-loader": "^7.3.3",
    "prettier": "^2.8.8",
    "react-refresh": "^0.14.0",
    "sass": "^1.63.6",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.3.3",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.85.0",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@askcodebase/common": "^1.1.0",
    "@hediet/node-reload": "^0.7.3",
    "@hediet/std": "^0.6.0",
    "@monaco-editor/react": "^4.5.1",
    "@tabler/icons-react": "^2.30.0",
    "@types/react-dom": "^18.2.6",
    "@types/react-window": "^1.8.5",
    "@vscode/codicons": "^0.0.33",
    "@vscode/webview-ui-toolkit": "^1.2.2",
    "classnames": "^2.3.2",
    "events": "^3.3.0",
    "jotai": "^2.2.2",
    "jotai-devtools": "^0.6.0",
    "monaco-editor": "^0.39.0",
    "node-fetch": "^3.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.7",
    "react-syntax-highlighter": "^15.5.0",
    "react-tooltip": "^5.18.0",
    "react-window": "^1.8.9",
    "rehype-mathjax": "^4.0.3",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "uuid": "^9.0.0",
    "xterm": "^5.2.1",
    "xterm-addon-canvas": "^0.4.0",
    "xterm-addon-fit": "^0.7.0"
  }
}
